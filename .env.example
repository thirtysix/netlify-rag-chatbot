# Example .env file - Copy this to .env and fill in your values

# REQUIRED: Neon Project ID (for deploy-new-rag.ts script)
NEON_PROJECT_ID=your-neon-project-id

# Base URL for RAG databases (connection string without database name)
DATABASE_URL_BASE=postgresql://user:password@ep-abc123.neon.tech

# REQUIRED: Your jobs database URL
# Create this database on Neon.tech first, then copy the connection string here
JOBS_DATABASE_URL=postgresql://user:password@ep-xyz456.neon.tech/rag-jobs-db

# Your DeepInfra API key (for LLM queries)
DEEPINFRA_API_KEY=your_deepinfra_api_key_here

# Optional: Netlify-specific settings
# NETLIFY_DEV=true